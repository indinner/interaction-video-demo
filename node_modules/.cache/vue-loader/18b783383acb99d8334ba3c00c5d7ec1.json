let player=Videojs(document.getElementById('myVideo'),this.videoPlayerOption,()=>{\n      console.log(\"视频准备完成\")\n    })\n\n    player.on('timeupdate',()=>{\n      let currentTime=player.currentTime();\n      console.log(currentTime)\n    })\n\n  }\n\n}\n</script>\n\n<style>\n.video-box{\n  width: 500px;\n  height: 300px;\n}\n</style>\n"]}]}